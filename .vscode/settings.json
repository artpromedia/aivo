{
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": ".github/workflows/*.yml",
    "https://raw.githubusercontent.com/compose-spec/compose-spec/master/schema/compose-spec.json": "infra/compose/*.yml"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.eol": "\n",
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "python.defaultInterpreterPath": "./.venv/Scripts/python.exe",
  "python.analysis.extraPaths": [
    "./services/hello-svc"
  ],
  "python.analysis.autoSearchPaths": true,
  "eslint.workingDirectories": [
    {
      "directory": "./services/notification-svc/nodejs-email-template",
      "changeProcessCWD": true
    }
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "typescript.validate.enable": false
  },
  "files.exclude": {
    "**/.eslintrc.js": true,
    "**/services/notification-svc/nodejs-email-template/.eslintrc.js": true
  },
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "typescript.suggest.autoImports": false,
  "typescript.validate.enable": true,
  "typescript.preferences.exclude": [
    "**/services/notification-svc/nodejs-email-template/**"
  ]
}
